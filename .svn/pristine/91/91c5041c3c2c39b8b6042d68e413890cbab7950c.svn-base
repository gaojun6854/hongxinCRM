<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 所有用户信息  -->
	<package name="pactPackage" extends="struts2" namespace="/pact">
	<!--需要拦截登录拦截 让它们入 "struts2"-->
		<action name="pactInfo" class="com.hongxin.action.OA.PactInfoAction">  
            <result name="success">/pact/findAllEligibleCustom.jsp</result><!-- 先查询可以购买的客户 -->
            <result name="showProducts">/pact/showProducts.jsp</result><!-- 带用户到达所有产品信息页面 -->
            <result name="onLineReviews">/pact/onLineReviews.jsp</result><!-- 查询所有线上线下需要审核的合约 -->
            
            <result name="getPactInfo4offline">/pact/getPactInfo4offline.jsp</result><!-- 线下合同信息 -->
            <result name="getPactInfo4online">/pact/getPactInfo4online.jsp</result><!-- 线上合同信息 -->
            <result name="getPactInfo">/pact/getPactInfo.jsp</result><!-- 合同信息查询 -->
             <result name="pactRecheck">/pact/getPactInfo4PactRecheck.jsp</result><!-- 合同复审 -->
            <result name="getPactInfo4PZoffline">/pact/getPactInfo4PZoffline.jsp</result><!-- 线下合同凭证审核 -->
            
             <result name="getPactInfo4Repayment">/pact/getPactInfo4Repayment.jsp</result><!-- 还款管理 -->
             <result name="findPactInfoList">/pact/findPactInfoList.jsp</result><!-- 查询合同集合 -->
            
            <result name="toPactRecheck">/pact/toPactRecheck.jsp</result><!-- 查询所有的复审信息 -->
            <result name="redirectPicUpload">/uploadify/fileup.jsp</result>
            <result name="repaymentCustom">/pact/repaymentCustom.jsp</result><!-- 查询还款信息 -->
            <result name="zhishuInsert">/pact/zhishuInsert.jsp</result>
            <result name="zhishuquery">/pact/zhishuquery.jsp</result>
            <result name="findPactNum">/pact/findPactInfo.jsp</result><!--合同号查询合约  -->
            <result name="findReimbursementToCustom">/pact/findReimbursementToCustom.jsp</result>
            <result name="RepurchaseDetails">/pact/RepurchaseDetails.jsp</result><!-- 回购详情 -->
            
            <!-- 拦截器 -->
            <interceptor-ref name="myStack"></interceptor-ref>
            <interceptor-ref name="myLogStack"></interceptor-ref>
            <!--如果不需要拦截 让它们已入 defaultStack-->
        </action>
	</package>
</struts>  

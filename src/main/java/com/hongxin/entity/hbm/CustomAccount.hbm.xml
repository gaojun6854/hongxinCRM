<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hongxin.entity">
	<class name="CustomAccount" table="t_custom_account">
     <id name="customId" type="string">
            <column name="custom_id" length="40" />
            <generator class="assigned" />
        </id>
            <property name="phoneNum" type="string">
                <column name="phone_num" length="18" />
            </property>
            <property name="paperType" type="java.lang.Character">
                <column name="paper_type" length="1" />
            </property>
            <property name="accountBank" type="string">
                <column name="account_bank" length="32" />
            </property>
            <property name="accountStart" type="java.lang.Character">
                <column name="account_start" length="1" />
            </property>
            <property name="accountFuyou" type="string">
                <column name="account_fuyou" length="32" />
            </property>
            <property name="bankHead" type="string">
                <column name="bank_head" length="256" />
            </property>
            <property name="payBank" type="string">
                <column name="pay_bank" length="20" />
            </property>
            <property name="payBankName" type="string">
                <column name="pay_bank_name" length="256" />
            </property>
            <property name="chageStaffNum" type="string">
                <column name="chage_staff_num" length="10" />
            </property>
            <property name="staffNum" type="string">
                <column name="staff_num" length="10" />
            </property>
            <property name="checkId" type="string">
                <column name="check_id" length="10" />
            </property>
            <property name="checkIdSer" type="java.lang.Integer">
                <column name="check_id_ser" />
            </property>
            <property name="checkIdDesc" type="string">
                <column name="check_id_desc" length="256" />
            </property>
            <property name="remark1" type="string">
                <column name="remark1" length="32" />
            </property>
            <property name="remark2" type="string">
                <column name="remark2" length="64" />
            </property>
    </class>
</hibernate-mapping>

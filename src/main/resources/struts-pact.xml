<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC  
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 所有用户信息  -->
	<package name="pactPackage" extends="struts2" namespace="/pact">
	<!--需要拦截登录拦截 让它们入 "struts2"-->
		<action name="pactInfo" class="com.hongxin.action.OA.PactInfoAction">  
            <result name="success">/pact/findAllEligibleCustom.jsp</result><!-- 先查询可以购买的客户 -->
            <result name="addPactInfo">/pact/addPactInfo.jsp</result><!-- 带用户到达购买  -->
            <result name="FirstCheck">/pact/onLineReviews.jsp</result><!-- 查询所有初审列表 -->
            <result name="redirectPicUpload">/uploadify/fileup.jsp</result>
            <result name="toPactRecheck">/pact/toPactRecheck.jsp</result><!-- 查询所有的复审信息 -->
            <result name="findPactInfoList">/pact/findPactInfoList.jsp</result><!-- 查询合同集合 -->
            <result name="updateFailPactInfo">/pact/updateFailPactInfo.jsp</result><!-- 更新失败合同 -->
            <result name="HGPactInfo">/pact/HGPactInfo.jsp</result><!-- 回购页面 -->
            <result name="pactHGForPaymentNotice">/paymentNotice/pactHGForPaymentNotice.jsp</result><!-- 还款通知内的回购 -->
            <result name="findReimbursementToCustom">/pact/findReimbursementToCustom.jsp</result>
            <result name="findFailPact">/pact/findFailPact.jsp</result><!-- 查询失败合同信息 -->
            <result name="findPactNum">/pact/findPactInfo.jsp</result><!--合同号查询合约  -->
            <result name="findCheckReceipt">/pact/findCheckReceipt.jsp</result><!-- 合同凭证信息 -->
            <result name="getPactInfo4online">/pact/getPactInfo4online.jsp</result><!-- 线上合同信息 -->
             <result name="pactRecheck">/pact/getPactInfo4PactRecheck.jsp</result><!-- 合同复审 -->
            <result name="findPactInfo">/pact/findPactInfo.jsp</result>
            <result name="AllgetMoneyList">/pact/all_getMoney_List.jsp</result>
            <result name="getMoney">/pact/getPactInfo_getMoney.jsp</result>
            <!-- 拦截器 -->
            <interceptor-ref name="myStack"></interceptor-ref>
            <interceptor-ref name="myLogStack"></interceptor-ref>
            <!--如果不需要拦截 让它们已入 defaultStack-->
        </action>
	</package>
</struts>  

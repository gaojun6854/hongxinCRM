<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hongxin.entity">
	<class name="CheckReceipts" table="t_check_receipts">
        <id name="recNum" column="rec_num" type="string">
    		<generator class="assigned" />
    	</id>
            <property name="recId" type="string">
                <column name="rec_id" length="16" />
            </property>
            <property name="recSer" type="java.lang.Integer">
                <column name="rec_ser" />
            </property>
            <property name="recDesc" type="string">
                <column name="rec_desc" length="64" />
            </property>
            <property name="recType" type="string">
                <column name="rec_type" length="1" />
            </property>
            <property name="recInfo" type="string">
                <column name="rec_info" />
            </property>
            <property name="managerNum" type="string">
                <column name="manager_num" length="16" />
            </property>
            <property name="managerName" type="string">
                <column name="manager_name" length="64" />
            </property>
            <property name="recManDesc" type="string">
                <column name="rec_man_desc" length="128" />
            </property>
            <property name="recPath" type="string">
                <column name="rec_path" length="64" />
            </property>
            <property name="recFile" type="string">
                <column name="rec_file" length="64" />
            </property>
            <property name="recRealFile" type="string">
                <column name="rec_real_file" length="64" />
            </property>
            <property name="createDate" type="string">
                <column name="create_date" length="256" />
            </property>
            <property name="createTime" type="string">
                <column name="create_time" length="256" />
            </property>
            <property name="remark1" type="string">
                <column name="remark1" length="32" />
            </property>
            <property name="remark2" type="string">
                <column name="remark2" length="64" />
            </property>
    </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hongxin.entity">
	<class name="CustomStatus" table="t_custom_status">

    <id name="customId" column="custom_id" >
      <generator class="assigned" />
    </id>

    <property name="custStart" type="java.lang.Character">
      <column name="cust_start"  not-null="false" />
    </property>
    <property name="custCheckStart" type="java.lang.Character">
      <column name="cust_check_start" not-null="false" />
    </property>
    <property name="checkId" type="string">
      <column name="check_id"  not-null="false" />
    </property>
    <property name="checkIdSer" type="integer">
      <column name="check_id_ser" not-null="false" />
    </property>
    <property name="signDate" type="string">
      <column name="sign_date" length="8" not-null="false" />
    </property>
    <property name="SignTime" type="string">
      <column name="sign_time" length="8" not-null="false" />
    </property>
    <property name="manageId" type="string">
      <column name="manage_id" length="10" not-null="false" />
    </property>
    <property name="mamageName" type="string">
      <column name="mamage_name" length="64" not-null="false" />
    </property>
    <property name="remark1" type="string">
      <column name="remark1" length="32" not-null="false" />
    </property>
    <property name="remark2" type="string">
      <column name="remark2" length="64" not-null="false" />
    </property>
  </class>
</hibernate-mapping>